<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.utsakb.amxy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/lv"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/lv_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.utsakb.amxy.view.FriendAddUpdateActivity"></activity>
        <activity android:name="com.utsakb.amxy.MainActivity"></activity>
        <activity android:name="com.utsakb.amxy.view.SplashScreenActivity"></activity>
        <activity android:name="com.utsakb.amxy.view.OnBoardSliderActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>